{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/main",
      "dest": "/main.html"
    },
    {
      "src": "/memories",
      "dest": "/memories.html"
    }
  ]
}